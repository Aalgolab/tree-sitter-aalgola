{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "aalgola",
      "camelcase": "Aalgola",
      "title": "Aalgola",
      "scope": "source.aalgola",
      "file-types": [
        "aason",
        "aalgola"
      ],
      "injection-regex": "^aalgola$",
      "class-name": "TreeSitterAalgola"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Syntax Highlighting for the Aalgola Programming Language",
    "authors": [
      {
        "name": "Aalgolab GmbH",
        "email": "info@aalgolab.de",
        "url": "https://aalgolab.de/"
      }
    ],
    "links": {
      "repository": "https://github.com/Aalgolab/tree-sitter-aalgola"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}